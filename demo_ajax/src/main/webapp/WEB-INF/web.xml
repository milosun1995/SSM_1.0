<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>VerifyImageDisplayServlet</servlet-name>
		<servlet-class>com.hitrust.servlet.VerifyImageDisplayServlet</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>VerifyImageDisplayForLoginServlet</servlet-name>
		<servlet-class>com.hitrust.servlet.VerifyImageDisplayForLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VerifyImageDisplayServlet</servlet-name>
		<url-pattern>/VerifyImage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>VerifyImageDisplayForLoginServlet</servlet-name>
		<url-pattern>/VerifyImageForLogin</url-pattern>
	</servlet-mapping>
</web-app>
